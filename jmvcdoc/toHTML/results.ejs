<%  
   var previous = "", res, current, title;
%>
<div id='selected'>
    <% for(var i =0; i < selected.length; i++){%>
		<%  current = selected[i];
 			title = (current.title ? current.title: current.name) || "";
			res = calculateDisplay(previous, title);
			name = normalizeName(current.name) %>
		<div class="content">
		    <a href="<%=name%>.html" class='selected choice <%=current.type%>' style="padding-left: <%= res.length * 20%>px">
		    	<span class='remove' title="close"></span>
				<%=res.name.replace("jQuery","$")%>
				
			</a>
		<% previous = title%>
		</div>
		<div class="bottomCorner">
			<div class="right"></div>
			<div class="left"></div>
		</div>
			<% if(i<(selected.length-1)){ %>
		<div class="spacer"><div>&nbsp;</div></div>
			<%}%>
	<%}%>
</div>
<div id='results' style="display: <%= hide? 'none' : 'block'%>">


	<div class="content">
	    <% for(var i =0; i < list.length; i++){%>
			<% current = list[i];
			   if(current.hide){ continue; }
			   title = (current.title ? current.title: current.name);
	           res = calculateDisplay(previous, title);
			   name = normalizeName(current.name); %>
		    <a href="<%=current.type == 'prototype' || current.type == 'static' ? 'javascript://': name%>.html" class='result choice <%=current.type%>' style="padding-left: <%= res.length * 20%>px">
		    	<%=res.name.replace("jQuery","$")%>
			</a>
			<% previous = title%>
		<%}%>
	</div>
		<div class="bottomCorner">
			<div class="right"></div>
			<div class="left"></div>
		</div>

</div>


